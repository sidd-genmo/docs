  "paths": {
    "/create": {
      "post": {
        "summary": "Create a new video generation task",
        "tags": ["video"],
        "description": "Initiates a new video generation task based on the provided prompt and arguments",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": ["tool", "prompt"],
                "properties": {
                  "tool": {
                    "type": "string",
                    "description": "Must be 'generateVideo'",
                    "enum": ["generateVideo"]
                  },
                  "prompt": {
                    "type": "string",
                    "description": "Text prompt describing the video to generate"
                  },
                  "webhookUrl": {
                    "type": "string",
                    "description": "Optional webhook URL for status updates",
                    "format": "uri"
                  },
                  "priority": {
                    "type": "string",
                    "enum": ["INTERACTIVE", "BATCH"],
                    "default": "INTERACTIVE",
                    "description": "Priority level for the generation"
                  },
                  "args": {
                    "type": "object",
                    "properties": {
                      "useMochi": {
                        "type": "boolean",
                        "description": "Whether to use Mochi model",
                        "default": false
                      },
                      "mochiArgs": {
                        "type": "object",
                        "properties": {
                          "num_inference_steps": {
                            "type": "integer",
                            "minimum": 50,
                            "maximum": 200,
                            "default": 100,
                            "description": "Number of inference steps for Mochi model"
                          }
                        }
                      },
                      "stealthMode": {
                        "type": "boolean",
                        "description": "Whether to make the generation private",
                        "default": false
                      },
                      "autopromptEnabled": {
                        "type": "boolean",
                        "description": "Whether to enable automatic prompt refinement",
                        "default": true
                      }
                    }
                  }
                }
              }
            }
          },
          "required": true
        },
        "security": [
          {
            "apiKey": []
          }
        ],
        "responses": {
          "202": {
            "description": "Video generation task created successfully",
            "content": {
              "application/json": {
                "schema": {
                  "description": "Video generation task created successfully",
                  "type": "object",
                  "properties": {
                    "taskId": {
                      "type": "string",
                      "description": "ID of the created generation task"
                    },
                    "statusUrl": {
                      "type": "string",
                      "description": "URL to check the status of the generation"
                    },
                    "text": {
                      "type": "string",
                      "description": "Human readable status message"
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "Default Response",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "error": {
                      "type": "string",
                      "description": "Error code"
                    },
                    "errorMessage": {
                      "type": "string",
                      "description": "Human readable error message"
                    }
                  }
                }
              }
            }
          },
          "401": {
            "description": "Default Response",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "error": {
                      "type": "string",
                      "description": "Error code"
                    },
                    "errorMessage": {
                      "type": "string",
                      "description": "Human readable error message"
                    }
                  }
                }
              }
            }
          },
          "500": {
            "description": "Default Response",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "error": {
                      "type": "string",
                      "description": "Error code"
                    },
                    "errorMessage": {
                      "type": "string",
                      "description": "Human readable error message"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/status/{taskId}": {
      "get": {
        "summary": "Get video generation task status",
        "tags": ["video"],
        "description": "Retrieve the current status of a video generation task",
        "parameters": [
          {
            "schema": {
              "type": "string"
            },
            "in": "path",
            "name": "taskId",
            "required": true,
            "description": "The ID of the task to check"
          }
        ],
        "responses": {
          "200": {
            "description": "Default Response",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "id": {
                      "type": "string",
                      "description": "Task ID"
                    },
                    "status": {
                      "type": "string",
                      "enum": [
                        "pending",
                        "processing",
                        "completed",
                        "failed",
                        "moderated"
                      ],
                      "description": "Current status of the task"
                    },
                    "progress": {
                      "type": "number",
                      "minimum": 0,
                      "maximum": 1,
                      "description": "Progress of the generation (0-1)"
                    },
                    "resultUrl": {
                      "type": "string",
                      "description": "URL of the generated video (when completed)"
                    },
                    "error": {
                      "type": "string",
                      "description": "Error message if the task failed"
                    },
                    "lastStatusAt": {
                      "type": "string",
                      "format": "date-time",
                      "description": "Timestamp of the last status update"
                    }
                  }
                }
              }
            }
          },
          "404": {
            "description": "Default Response",
            "content": {
