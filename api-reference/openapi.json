{
  "openapi": "3.0.3",
  "info": {
    "title": "Genmo API",
    "description": "API for video generation and processing",
    "version": "1.0.0"
  },
  "components": {
    "securitySchemes": {
      "apiKey": {
        "type": "apiKey",
        "name": "Authorization",
        "in": "header",
        "description": "API key for authentication"
      }
    },
    "schemas": {
      "Error": {
        "type": "object",
        "properties": {
          "error": {
            "type": "string",
            "description": "Error code"
          },
          "errorMessage": {
            "type": "string",
            "description": "Human readable error message"
          }
        }
      },
      "VideoGenerationRequest": {
        "type": "object",
        "required": ["tool", "prompt"],
        "properties": {
          "tool": {
            "type": "string",
            "enum": ["generateVideo"],
            "description": "Must be 'generateVideo'"
          },
          "prompt": {
            "type": "string",
            "description": "Text prompt describing the video to generate"
          },
          "webhookUrl": {
            "type": "string",
            "description": "Optional webhook URL for status updates",
            "format": "uri"
          },
          "priority": {
            "type": "string",
            "enum": ["INTERACTIVE", "BATCH"],
            "default": "INTERACTIVE",
            "description": "Priority level for the generation"
          },
          "args": {
            "type": "object",
            "properties": {
              "useMochi": {
                "type": "boolean",
                "description": "Whether to use Mochi model",
                "default": false
              },
              "mochiArgs": {
                "type": "object",
                "properties": {
                  "num_inference_steps": {
                    "type": "integer",
                    "minimum": 50,
                    "maximum": 200,
                    "default": 100,
                    "description": "Number of inference steps for Mochi model"
                  }
                }
              },
              "stealthMode": {
                "type": "boolean",
                "description": "Whether to make the generation private",
                "default": false
              },
              "autopromptEnabled": {
                "type": "boolean",
                "description": "Whether to enable automatic prompt refinement",
                "default": true
              }
            }
          }
        }
      },
      "VideoGenerationResponse": {
        "type": "object",
        "properties": {
          "taskId": {
            "type": "string",
            "description": "ID of the created generation task"
          },
          "statusUrl": {
            "type": "string",
            "description": "URL to check the status of the generation"
          },
          "text": {
            "type": "string",
            "description": "Human readable status message"
          }
        }
      },
      "TaskStatus": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "description": "Task ID"
          },
          "status": {
            "type": "string",
            "enum": [
              "pending",
              "processing",
              "completed",
              "failed",
              "moderated"
            ],
            "description": "Current status of the task"
          },
          "progress": {
            "type": "number",
            "minimum": 0,
            "maximum": 1,
            "description": "Progress of the generation (0-1)"
          },
          "resultUrl": {
            "type": "string",
            "description": "URL of the generated video (when completed)"
          },
          "error": {
            "type": "string",
            "description": "Error message if the task failed"
          },
          "lastStatusAt": {
            "type": "string",
            "format": "date-time",
            "description": "Timestamp of the last status update"
          }
        }
      },
